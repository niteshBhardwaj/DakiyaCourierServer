{
  "name": "dakiya-courier-server",
  "description": "Dakiya node js server",
  "prisma": {
    "seed": "ts-node prisma/seed"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check:types": "tsc --noEmit",
    "build": "tsc && tsc-alias",
    "build-i": "npm install && npm run generate && tsc && tsc-alias",
    "build:docker:prod": "docker build . -t my-fastify-app --build-arg APP_ENV=production",
    "start": "node build/app",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/app.ts",
    "bun-dev": "bun --watch src/app.ts",
    "dev-inspect": "npm run build && node --inspect build/app",
    "pretest": "npm run build",
    "generate": "bunx prisma format && npx prisma generate",
    "seed": "bunx prisma db seed",
    "studio": "bunx prisma studio --port 5000 --browser none",
    "rebuild-bcrypt": "npm rebuild bcrypt",
    "test": "NODE_ENV=test tap",
    "test:watch": "NODE_ENV=test tap -w"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "nodejs",
    "fastify",
    "typescript"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run check:types"
    }
  },
  "peerDependencies": {
    "graphql": "^16.8.1"
  },
  "author": "Nitesh Bhardwaj",
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.2",
    "@types/node-fetch": "^3.0.3",
    "@types/tap": "^15.0.11",
    "@types/validator": "^13.11.6",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^9.0.11",
    "pino-pretty": "^10.2.3",
    "prettier": "^3.1.0",
    "prisma": "^5.10.2",
    "tap": "^18.6.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@as-integrations/fastify": "^2.1.1",
    "@prisma/client": "^5.10.2",
    "@sinclair/typebox": "^0.31.26",
    "@types/nodemailer": "^6.4.14",
    "ajv": "^8.12.0",
    "bcrypt": "^5.1.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "env-schema": "^5.2.1",
    "event-dispatch": "^0.4.1",
    "fastify": "^4.24.3",
    "fastify-cookie": "^5.7.0",
    "fastify-now": "^3.1.0",
    "fastify-plugin": "^4.5.1",
    "graphql": "^16.8.1",
    "graphql-type-json": "^0.3.2",
    "jsonata": "^2.0.4",
    "jsonwebtoken": "^9.0.2",
    "keygen": "^0.4.0",
    "nodemailer": "^6.9.7",
    "otp-generator": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "2.0.0-beta.3",
    "typedi": "^0.10.0",
    "validator": "^13.11.0",
    "winston": "^3.11.0"
  }
}